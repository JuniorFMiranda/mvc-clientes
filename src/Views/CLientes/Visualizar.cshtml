@model MvcClientes.Models.Cliente;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @{
        ViewData["Title"] = "Visualizando cliente";
    }
</head>
<body>
    <div class="context">
        <header>
            <h2>@ViewData["Title"]</h2>
        </header>
        <main>
            <form>
                @await Html.PartialAsync("_Crud")
                
                <div class="form-group">
                    <button asp-action="Index">Voltar</button>
                </div>    
            </form>    
        </main>
    </div>    
</body>
</html>